{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/*": ["src/*"],
      "@core/*": ["src/core/*"],
      "@config/*": ["src/core/IConfiguraion/*"],
      "@database/*": ["src/core/database/*"],
      "@schemas/*": ["src/core/database/schemas/*"],
      "@enums/*": ["src/core/enums/*"],
      "@swagger/*": ["src/core/swagger/*"],
      "@cors/*": ["src/core/cors/*"],
      "@interceptors/*": ["src/core/interceptors/*"],
      "@filters/*": ["src/core/filters/*"],
      "@pipes/*": ["src/core/pipes/*"],
      "@modules/*": ["src/modules/*"],
      "@auth/*": ["src/modules/auth/*"],
      "@mail/*": ["src/modules/mail/*"],
      "@otp/*": ["src/modules/otp/*"],
      "@whatsapp/*": ["src/modules/whatsapp/*"]
    },
  }
}
